* {
  background-color: #e5e9f0;
  text-color: #4c566a;
  spacing: 0;
  width: 512px;
}

window {
  border: 5;
  border-color: #81a1c1;
}

inputbar {
  border: 0 0 1px 0;
  children: [prompt,entry];
}

prompt {
  padding: 16px;
  border: 0 1px 0 0;
}

entry {
  padding: 16px;
}

listview {
  cycle: false;
  margin: 0 0 -1px 0;
  scrollbar: false;
}

element {
  border: 0 0 1px 0;
  padding: 16px;
}

element selected {
  background-color: #d8dee9;
  text-color: #d08770;
}
