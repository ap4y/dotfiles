borderwidth 6
gap 20 20 20 20
snapdist 20

color activeborder "#65737e"
color inactiveborder "#343d46"
color groupborder "#4f5b66"

color font "#4f5b66"
color selfont "#343d46"
color menubg "#2b303b"
color menufg "#c0c5ce"

fontname "tewi:pixelsize=11"

sticky yes

command term /usr/local/bin/urxvtc
command lock /usr/local/bin/slock

bind 4-Return terminal
bind 4-e "emacsclient -c"
bind 4-b "tabbed -c vimb -e"
bind 4-c "urxvtc -e weechat"

bind 4-h htile
bind 4-v vtile
bind M4-h hmaximize
bind M4-v vmaximize
